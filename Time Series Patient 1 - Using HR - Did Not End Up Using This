    "    y_test = np.array(test_data['HRV']).reshape(-1,1)\n",
    "    \n",
    "    ma_model = ARIMA(y_train, order=(0,0,o))\n",
    "    ma_results = ma_model.fit()\n",
    "    \n",
    "    # make predictions\n",
    "    pred_data = ma_results.predict(start=len(y_train), end=len(y_train) + len(y_test) - 1)\n",
